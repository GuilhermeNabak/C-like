def main():
    acc = 0  # Inicializa um contador

    for i in range(6):
        jogos = input().strip()  # VariÃ¡vel recebe os caracteres
        if jogos == 'V':  # Se venceu, adiciona 1 ao contador
            acc += 1

    if acc > 4:  # Se o contador for maior que 4, imprime 1
        print("1")
    elif 2 < acc < 5:  # Se o contador for maior que 2 e menor que 5, imprime 2
        print("2")
    elif 0 < acc < 3:  # Se o contador for maior que 0 e menor que 3, imprime 3
        print("3")
    else:  # Se o contador for igual a 0, imprime -1
        print("-1")

if __name__ == "__main__":
    main()
